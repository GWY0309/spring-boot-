<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.badmintonsystem.mapper.RacketRentalMapper">

    <insert id="insert" useGeneratedKeys="true" keyProperty="id">
        INSERT INTO t_racket_rental (racket_id, user_id, rental_time, status)
        VALUES (#{racketId}, #{userId}, NOW(), 0)
    </insert>

</mapper>